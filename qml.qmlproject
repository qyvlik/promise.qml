import QmlProject 1.1

Project {
    mainFile: "main.qml"

    /* Include .qml, .js, and image files from current directory and subdirectories */
    QmlFiles {
        directory: "."
    }
    JavaScriptFiles {
        directory: "."
    }
    ImageFiles {
        directory: "."
    }
    Files{
        directory: "."
        filter: "*.md"
    }
    Files{
        directory: "."
        filter: "qmldir"
    }
    Files{
        directory: "../docs"
        filter: "*.md"
    }

    /* List of plugin directories passed to QML runtime */
    importPaths: [ "." ]
}
